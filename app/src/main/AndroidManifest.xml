    xmlns:tools="http://schemas.android.com/tools"
    package="com.coinshot.uilibrary2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpandableActivity"></activity>
        <activity android:name=".DatePickerActivity"/>
        <activity android:name=".MaterialFirstActivity"/>
        <activity android:name=".MaterialSecondActivity">

        </activity>
        <activity android:name=".MaterialThirdActivity"/>
        <activity android:name=".BootstrapFirstActivity"/>
        <activity android:name=".BootstrapSecondActivity"/>
        <activity android:name=".BootstrapThirdActivity"/>
        <activity android:name=".SmarttabActivity"/>
    </application>

</manifest>